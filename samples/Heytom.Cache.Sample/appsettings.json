{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "HybridCache": {
    "EnableLocalCache": true,
    "LocalCacheMaxSize": 1000,
    "LocalCacheDefaultExpiration": "00:05:00",
    "RedisOperationTimeout": "00:00:05",
    "EnableMetrics": true
  }
}
